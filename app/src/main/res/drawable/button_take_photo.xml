<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#00ffffff"/>
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"/>
            <stroke
                android:width="3dp"
                android:color="@color/app_primary"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#33FFFFFF"/>
            <size
                android:width="100dp"
                android:height="100dp"/>
        </shape>

    </item>
</layer-list>